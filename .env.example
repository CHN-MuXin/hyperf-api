APP_NAME=hyperf-api
APP_ENV=dev
APP_REQUEST_LOG=true
APP_RESPONSE_LOG=true
APP_LOG=true
HF_LOG=true
APP_TEST=false

SERVER_HOST_HTTP=0.0.0.0
SERVER_PORT_HTTP=9501
SERVER_HOST_WS=0.0.0.0
SERVER_PORT_WS=9502

# Hyperf 进程的最大执行时间 适用于执行超长task
MAX_WAIT_TIME=600

DB_DRIVER=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=
DB_USERNAME=
DB_PASSWORD=
DB_CHARSET=utf8mb4
DB_COLLATION=utf8mb4_unicode_ci

DB_PREFIX=
REDIS_HOST=127.0.0.1
REDIS_AUTH=(null)
REDIS_PORT=6379
REDIS_DB=0

JWT_SECRET=hyperf-api
JWT_TTL=7200
JWT_TTL_CACHE=3600

# 文件驱动方式 local 具体参考 config/autoload/file.php
FILE_TYPE=local
# 文件上传目录 相对于项目根目录
UPLOAD_PATH=uploads

OSS_URL=
OSS_ACCESS_ID=
OSS_ACCESS_SECRET=
OSS_BUCKET=
OSS_ENDPOINT=

# 高德地图IP定位key
FreeApiKey=